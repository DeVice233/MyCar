<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Images\car.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\centurygothic.ttf" />
    <None Remove="Fonts\centurygothic_bold.ttf" />
    <None Remove="Fonts\fontawesome-webfont.ttf" />
    <None Remove="Images\add.png" />
    <None Remove="Images\car_256.png" />
    <None Remove="Images\car_64px.png" />
    <None Remove="Images\catalog_64px.png" />
    <None Remove="Images\mark_64px.png" />
    <None Remove="Images\user_64px.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Images\car.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PropertyChanged.Fody" Version="4.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ModelsApi\ModelsApi.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\centurygothic.ttf" />
    <Resource Include="Fonts\centurygothic_bold.ttf" />
    <Resource Include="Fonts\fontawesome-webfont.ttf" />
    <Resource Include="Images\add.png" />
    <Resource Include="Images\car_256.png" />
    <Resource Include="Images\car_64px.png" />
    <Resource Include="Images\catalog_64px.png" />
    <Resource Include="Images\mark_64px.png" />
    <Resource Include="Images\user_64px.png" />
  </ItemGroup>

</Project>
